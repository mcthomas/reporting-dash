<body style="background-color:#C0C0C0;">
<div class="container">
<table class="responsive-table highlight">
  <thead>
  <tr>
    <th>Channel</th>
    <th>Name</th>
    <th>Structure</th>
    <th>Tone</th>
    <th>Desired Action</th>
  </tr>
  </thead>
  <h5><u class="purple-text">Channel Plans</u></h5>
  <tbody>
  <% @channelplans.each_with_index do |channelplan, index|%>
  <tr>
    <td><span class="truncate" style="display: inline-block; width: 130px;"><%=channelplan.channel %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 130px;"><%=channelplan.name %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 130px;"><%=channelplan.structure %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 130px;"><%=channelplan.tone %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 130px;"><%=channelplan.action %></span></td>
    <td><%= link_to "edit", edit_channelplan_path(channelplan),class:'btn btn-small yellow darken-2' %></td>
    <td><%= button_to "delete",channelplan_path(channelplan), method: :delete, form: {data: {turbo_confirm: "Are you sure you want to delete this channel plan?"}}, class:"btn btn-small red" %></td>
  </tr>
    <%end %>
  </tbody>
</table>
<br>
<a href="<%= new_channelplan_url %>" class="btn-small waves-effect waves-light green darken-1">
        <i class="material-icons">add</i>
      </a>
<br><br><br>
<table class="responsive-table highlight">
  <thead>
  <tr>
    <th>Users</th>
    <th>Users Active</th>
    <th>Users Inactive</th>
    <th>Users Validated</th>
    <th>Users Not Validated</th>
  </tr>
  </thead>
  <h5><u class="purple-text">User Reports</u></h5>
  <tbody>
<% @userreports.each_with_index do |userreport, index|%>
  <tr>
    <td><span class="truncate" style="display: inline-block; width: 108;"><%=userreport.title %></span></td>
    <td><span class="truncate" style="display: inline-block; width: 108;"><%=userreport.users %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 108;"><%=userreport.usersactive %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 108;"><%=userreport.usersinactive %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 108;"><%=userreport.usersvalidated %></span></td>
    <td ><span class="truncate" style="display: inline-block; width: 108;"><%=userreport.usersnotvalidated %></span></td>
    <td><%= link_to "edit", edit_userreport_path(userreport),class:'btn btn-small yellow darken-2' %></td>
    <td><%= button_to "delete",userreport_path(userreport), method: :delete, form: {data: {turbo_confirm: "Are you sure you want to delete this channel plan?"}}, class:"btn btn-small red" %></td>
  </tr>
  <%end %>
  </tbody>
</table>
<br>
<a href="<%= new_userreport_url %>" class="btn-small waves-effect waves-light green darken-1">
        <i class="material-icons">add</i>
      </a>
      <br><br><br>
      <table class="responsive-table highlight">
  <thead>
  <tr>
    <th>Title</th>
  </tr>
  </thead>
  <h5><u class="purple-text">Eligibility Lists</u></h5>
  <tbody>
<% @eligibilitylists.each_with_index do |eligibilitylist, index|%>
  <tr>
    <td><span class="truncate" style="display: inline-block; width: 650px;"><%=eligibilitylist.title %></span></td>
    <td><%= link_to "edit", edit_eligibilitylist_path(eligibilitylist),class:'btn btn-small yellow darken-2' %></td>
    <td><%= button_to "delete",eligibilitylist_path(eligibilitylist), method: :delete, form: {data: {turbo_confirm: "Are you sure you want to delete this channel plan?"}}, class:"btn btn-small red" %></td>
  </tr>
  <%end %>
  </tbody>
</table>
<br>
<a href="<%= new_eligibilitylist_url %>" class="btn-small waves-effect waves-light green darken-1">
        <i class="material-icons">add</i>
      </a>
      <br><br><br>
</div>
</body>